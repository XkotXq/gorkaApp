{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
	"jsx": "react",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
, "src/app/_layout.jsx", "src/app/(tabs)/_layout.jsx", "src/app/+not-found.jsx"  ]
}